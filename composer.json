{
    "name": "acseo/sylius-admin-tracker-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "audit",
        "logging",
        "user actions",
        "entity tracking"
    ],
    "description": "Sylius Plugin for logging and tracking user actions on entities",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sylius/sylius": "~1.11.0 || ~1.12.0",
        "symfony/security-bundle": "^5.3 || ^6.0",
        "doctrine/orm": "^2.10",
        "psr/log": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ACSEO\\SyliusAdminTrackerPlugin\\": "src/"
        }
    }
}
